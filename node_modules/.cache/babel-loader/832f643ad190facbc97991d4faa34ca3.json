,eAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA;AACA;AACA;AAdA,GArBA;AAsCA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,GAAA,GAAA,sHAAA;;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,KAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,eAAA,KAAA;AACA,OAFA,MAEA;AACA,eAAA,IAAA;AACA;AACA,KARA;AASA,IAAA,SATA,uBASA;AACA,UAAA,GAAA,GAAA,8FAAA;;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,KAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,eAAA,KAAA;AACA,OAFA,MAEA;AACA,eAAA,IAAA;AACA;AACA,KAhBA;AAiBA,IAAA,KAjBA,mBAiBA;AACA,aAAA,KAAA,IAAA,CAAA,IAAA,IAAA,CAAA,GAAA,OAAA,GAAA,sBAAA;AACA;AAnBA;AAtCA,CAAA","sourcesContent":["<template>\r\n    <div id=\"admin-page\">\r\n        <div class=\"v-wrapper\">\r\n            <div class=\"v-container\">\r\n                <h4>{{ title }}</h4>\r\n                <form @submit.prevent=\"sendForm()\">\r\n                    <input type=\"email\" :class=\"{'error': validEmail}\"  placeholder=\"Correo\" v-model=\"form.email\">\r\n                    <input type=\"password\" v-if=\"this.form.type == 0\" :class=\"{'error': validPass}\" placeholder=\"Contraseña\" v-model=\"form.pass\">\r\n                    <div class=\"button-box\">\r\n                        <a href=\"javascript:void(0)\" @click=\"form.type = 1\" v-if=\"form.type == 0\">Recuperar contraseña</a>\r\n                        <a href=\"javascript:void(0)\" @click=\"form.type = 0\" v-if=\"form.type == 1\">Iniciar sesión</a>\r\n                        <button class=\"v-btn v-btn-blue\" v-if=\"form.type == 0\">Iniciar sesión</button>\r\n                        <button class=\"v-btn v-btn-blue\" v-if=\"form.type == 1\">Recuperar contraseña</button>\r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: 'AdminPage',\r\n\r\n        data(){\r\n            return {\r\n                form:{\r\n                    email: '', // 0: Iniciar sesion 1: Recuperar contraseña\r\n                    pass: '',\r\n                    type: 0\r\n                }\r\n            }\r\n        },\r\n    \r\n        mounted(){\r\n            let header = document.getElementById('header')\r\n            let footer = document.getElementById('footer')\r\n            \r\n            header.style.display = \"none\"\r\n            footer.style.display = \"none\"\r\n        },\r\n\r\n        methods:{\r\n            sendForm(){\r\n                if(this.validType()){\r\n                    console.log(this.form);\r\n                }\r\n            },\r\n            validType(){\r\n                if(this.form.type == 0 && !this.validEmail && !this.validPass){\r\n                    return true;\r\n                }else if(this.form.type == 1 && !this.validEmail){\r\n                    return true;\r\n                } else{\r\n                    return false;\r\n                }\r\n            }\r\n        },\r\n\r\n        computed:{\r\n            validEmail(){\r\n                const exp = /^(([^<>()[\\]\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@(([^<>()[\\]\\.,;:\\s@\\\"]+\\.)+[^<>()[\\]\\.,;:\\s@\\\"]{2,})$/i\r\n                if(exp.test(this.form.email)){\r\n                    return false\r\n                }else{\r\n                    return true\r\n                }\r\n            },\r\n            validPass(){\r\n                const exp = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[$@$!%*?&#.$($)$-$_])[A-Za-z\\d$@$!%*?&#.$($)$-$_]{8,15}$/;\r\n                if(exp.test(this.form.pass)){\r\n                    return false\r\n                }else{\r\n                    return true\r\n                }\r\n            },\r\n            title(){\r\n                return (this.form.type==0)?'Login':'Recuperar contraseña';\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.v-navbar{\r\n    display: none!important;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components"}]}
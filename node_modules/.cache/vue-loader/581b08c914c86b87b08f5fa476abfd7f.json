te d-md-none collapsed flaticon-menu\" type=\"button\" ta-toggle=\"collapse\" data-target=\"sidebarMenu\" aria-controls=\"sidebarMenu\" aria-expanded=\"false\" ia-label=\"Toggle navigation\" @click=\"expandMenu($event)\">\r\n      </button>\r\n      <ul class=\"navbar-nav px-3 v-logOut\">\r\n        <li class=\"nav-item text-nowrap\">\r\n          <a class=\"nav-link\" @click=\"logOut()\">Cerrar sesi√≥n</a>\r\n        </li>\r\n      </ul>\r\n    </nav>\r\n    <div class=\"container-fluid\">\r\n      <div class=\"row\">\r\n        <nav class=\"col-md-3 col-lg-2 d-md-block bg-light v-sidebar\" ref=\"sidebarMenu\">\r\n          <div class=\"sidebar-sticky pt-3\">\r\n            <ul class=\"nav flex-column\">\r\n              <li class=\"nav-item\">\r\n                <a class=\"nav-link active\" href=\"#\" @click.prevent=\"selectComponent('0')\">\r\n                  <span data-feather=\"home\"></span>\r\n                  DASHBOARD <span class=\"sr-only\" >(current)</span>\r\n                </a>\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <a class=\"nav-link\" href=\"#\" @click.prevent=\"selectComponent('1')\">\r\n                  <span data-feather=\"file\"></span>\r\n                  CREAR CONTENIDO\r\n                </a>\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <a class=\"nav-link\" href=\"#\" @click.prevent=\"selectComponent('2')\">\r\n                  <span data-feather=\"shopping-cart\"></span>\r\n                  EDITAR CONTENIDO\r\n                </a>\r\n              </li>\r\n              <li class=\"nav-item\">\r\n                <a class=\"nav-link\" href=\"#\" @click.prevent=\"selectComponent('3')\">\r\n                  <span data-feather=\"users\"></span>\r\n                  EDITAR USUARIO\r\n                </a>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </nav>\r\n        <main role=\"main\" class=\"col-md-9 ml-sm-auto col-lg-10 px-md-4\">\r\n          <div class=\"d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 -3 border-bottom\">\r\n            <h1 class=\"h2\">Dashboard</h1>\r\n          </div>\r\n          \r\n          <transition name=\"fade-down\" mode=\"out-in\">\r\n            <component :is=\"currentCompon\"></component>\r\n          </transition>\r\n          \r\n          \r\n        </main>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport firebase from 'firebase'\r\nimport AdminCreateContent from './AdminCreateContent'\r\nimport AdminEditUser from './AdminEditUser'\r\nimport AdminEditContent from './AdminEditContent'\r\nimport AdminDashboard from './AdminDashboard'\r\nimport {mapState, mapMutations, mapGetters} from 'vuex'\r\n\r\nexport default {\r\n  name: 'AdminDashboardPage',\r\n  props:[\r\n      'AdminCreateContent',\r\n      'AdminEditUser',\r\n      'AdminDashboard',\r\n      'AdminEditContent',\r\n  ],\r\n  components:{\r\n    AdminCreateContent,\r\n    AdminEditUser,\r\n    AdminDashboard,\r\n    AdminEditContent,\r\n  },\r\n  data(){\r\n    return {\r\n      loaded: false,\r\n      allCompon: [\r\n        'AdminDashboard',\r\n        'AdminCreateContent',\r\n        'AdminEditContent',\r\n        'AdminEditUser',\r\n      ],\r\n      currentCompon: 'AdminDashboard',\r\n      dataName: [],\r\n    }\r\n  },\r\n  methods:{\r\n    ...mapMutations(['setUser', 'updateUser']),\r\n    logOut(){\r\n      firebase.auth().signOut().then(()=>{\r\n        this.$router.push({name: 'Admin'})\r\n      })\r\n    },\r\n    selectComponent(val){\r\n      this.currentCompon = this.allCompon[val]\r\n    },\r\n    expandMenu(event){\r\n      const refVal = event.target.getAttribute('data-target')\r\n\r\n      if(event.target.getAttribute('data-collapsed') === null){\r\n        event.target.setAttribute('data-collapsed', 'true')\r\n      }\r\n      \r\n      let isCollapse = event.target.getAttribute('data-collapsed')\r\n\r\n      if(isCollapse === 'true'){\r\n        this.$refs[`${refVal}`].style.maxHeight = '400px'\r\n        event.target.setAttribute('data-collapsed', 'false')\r\n      }else{\r\n        this.$refs[`${refVal}`].style.maxHeight = '0px'\r\n        event.target.setAttribute('data-collapsed', 'true')\r\n      }\r\n\r\n      window.addEventListener('resize', ()=>{\r\n        if(window.innerWidth >= 768){\r\n          this.$refs.sidebarMenu.style.maxHeight = 'none'\r\n          this.$refs.sidebarMenu.style.height = '100vh'\r\n          console.log(this.$refs.sidebarMenu.style.maxHeight);\r\n          console.log(object);\r\n        }else{\r\n          this.$refs.sidebarMenu.style.maxHeight = '0'\r\n          this.$refs.sidebarMenu.style.height = 'auto'\r\n        }\r\n      })\r\n\r\n    \r\n    }\r\n  },\r\n\r\n  computed:{\r\n    ...mapState(['user']),\r\n  },\r\n  \r\n  created(){\r\n    const data = setInterval(() => {\r\n      const user = firebase.auth().currentUser\r\n        if(user){\r\n          this.setUser(user);\r\n          this.dataName = user.displayName?user.displayName.split('|'):user.email\r\n          this.loaded = true;\r\n          clearTimeout(data)\r\n        }\r\n    }, 500);\r\n  },\r\n\r\n  watch:{\r\n    user(){\r\n      this.dataName = this.user.displayName.split('|')\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n  .admin-dashboard-page .navbar{\r\n    min-height: 100px;\r\n  }\r\n  \r\n  .admin-dashboard-page .bd-placeholder-img {\r\n    font-size: 1.125rem;\r\n    text-anchor: middle;\r\n    -webkit-user-select: none;\r\n    -moz-user-select: none;\r\n    -ms-user-select: none;\r\n    user-select: none;\r\n  }\r\n\r\n  .admin-dashboard-page .v-sidebar{\r\n    height: 100vh;\r\n    max-height: 100vh;\r\n    position: relative;\r\n  }\r\n\r\n  .admin-dashboard-page .navbar-toggler{\r\n    height: 50px;\r\n    width: 50px;\r\n    font-size: 30px;\r\n    padding: 5px;\r\n    right: 10px;\r\n  }\r\n\r\n  .admin-dashboard-page .navbar-brand{\r\n    width: auto;\r\n    white-space: normal;\r\n  }\r\n\r\n  .admin-dashboard-page p{\r\n    margin: 0;\r\n    color: white;\r\n  }\r\n\r\n  .admin-dashboard-page .icon:before{\r\n    font-size: 50px;\r\n  }\r\n\r\n  @media screen and (max-width: 768px) {\r\n    .admin-dashboard-page .bd-placeholder-img-lg {\r\n      font-size: 3.5rem;\r\n    }\r\n    \r\n    .admin-dashboard-page .v-logOut{\r\n      margin-right: 70px !important;\r\n    }\r\n\r\n    .admin-dashboard-page .v-sidebar{\r\n      position: relative;\r\n      max-height: 0;\r\n      height: auto ;\r\n      overflow: hidden; \r\n      transition: all 1s ease;\r\n    }\r\n  }\r\n</style>"]}]}
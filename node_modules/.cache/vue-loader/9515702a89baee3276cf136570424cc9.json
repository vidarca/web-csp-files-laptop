display: flex;\r\n    flex-direction: row;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n    right: 0;\r\n    max-width: 200px;\r\n    height: auto;\r\n    margin-right: 10px;\r\n    border: 0.5px solid black;\r\n    padding: 5px;\r\n    border-radius: 15px;\r\n    box-shadow: 0 0 6px black;\r\n    transition: all 0.6s ease;\r\n    transform: translateX(200px);\r\n    opacity: 0;\r\n  }\r\n  \r\n  .select-field{\r\n    margin: 5px 0;\r\n  }\r\n\r\n  .alert-box .icon-err{\r\n    color: white;\r\n    font-size: 15px;\r\n    font-weight: bolder;\r\n    background-color: rgb(200, 0, 0);\r\n    padding: 6px 9px 6px 10px;\r\n    border-radius: 50%;\r\n    margin-left: 5px;\r\n    box-shadow: 0px 0px 12px rgba(0,0,0,0.40);;\r\n  }\r\n\r\n  .alert-box .icon-succ{\r\n    color: white;\r\n    font-size: 17px;\r\n    font-weight: bolder;\r\n    background-color: rgb(0, 150, 0);\r\n    padding: 6px 9px 3px 10px;\r\n    border-radius: 50%;\r\n    margin-left: 5px;\r\n    box-shadow: 0px 0px 12px rgba(0,0,0,0.40);;\r\n  }\r\n\r\n  .alert-box p{\r\n    font-size: 13px;\r\n    font-weight: bolder;\r\n    margin: 0 5px;\r\n    text-align: center;\r\n  }\r\n\r\n  .alert-show{\r\n    transform: translateX(0px) !important;\r\n    opacity: 1 !important;\r\n  }\r\n\r\n  /* .form-class .info-label{\r\n    position: absolute;\r\n    left: 5px;\r\n    top: 0;\r\n  } */\r\n\r\n\r\n</style>"]}]}